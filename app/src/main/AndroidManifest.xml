<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.amor.csedu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".XYPlotActivity"
            android:label="XY Plot" />
        <activity
            android:name=".PieChartActivity"
            android:label="Pie Chart" />
        <activity
            android:name=".ShowXYPlotActivity"
            android:configChanges="orientation|keyboardHidden|screenSize" />
        <activity android:name=".ShowPiechartActivity" />
        <activity android:name=".ScatterPlotActivity" />
        <activity android:name=".ShowScatterPlotActivity" />
        <activity
            android:name=".Test"
            android:exported="true"
            android:icon="@drawable/ic_launcher">
            <intent-filter>
                <action android:name="android.intent.action.GET_CONTENT" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.OPENABLE" />

                <data android:mimeType="*/*" />
            </intent-filter>
        </activity>
        <activity android:name=".BarChartActivity" />
        <activity android:name=".ShowBarChartActivity" />
        <activity android:name=".BubbleChartActivity" />
        <activity android:name=".ShowBubbleChartActivity"></activity>
    </application>

</manifest>